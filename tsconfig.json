{
  "compilerOptions": {
    "resolveJsonModule": true,
    "target": "es2016",
    "module": "ESNext",
    "jsx": "react",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*", "custom.d.ts"]
}